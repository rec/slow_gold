command {
  type: CUT
  keypress: "command + x"
}
command {
  type: COPY
  keypress: "command + c"
}
command {
  type: PASTE
  keypress: "command + v"
}
command {
  type: SELECT_ALL
  keypress: "command + a"
}
command {
