  category: "Help"
  desc {
    menu: "Show About Window At Startup"
    full: "Toggle whether we show the About Window every time we start up."
  }
  is_global_setter: true
  address {
    part {
      name: "show_about_on_startup"
    }
  }
  setter_type_name: "rec.slow.GuiSettings"
}
command {
  type: TOGGLE_SHOW_CURSOR_LABELS
  category: "Display"
  desc {
    menu: "Show Loop Point Labels"
    full: "Show draggable labels for the loop points on the waveform display."
  }
  is_global_setter: true
  address {
    part {
      name: "show_cursor_labels"
    }
  }
  setter_type_name: "rec.widget.waveform.WaveformProto"
}
command {
  type: TOGGLE_SHOW_LABELS_AT_TOP
  category: "Display"
  desc {
    menu: "Show Loop Point Labels At Top"
    full: "Show the loop point labels at the top of the waveform display."
  }
  is_global_setter: true
  address {
    part {
      name: "show_labels_at_top"
    }
  }
  setter_type_name: "rec.widget.waveform.WaveformProto"
}
command {
  type: TOGGLE_SHOW_MASTER_TUNE
  category: "Display"
  desc {
    menu: "Show Master Tune Slider"
    full: "Show the Master Tune Slider in the Transform Panel."
  }
  is_global_setter: true
  address {
    part {
      name: "show_master_tune"
    }
  }
  setter_type_name: "rec.slow.GuiSettings"
}
command {
  type: TOGGLE_SHOW_SELECTION_BUTTONS
  category: "Display"
  desc {
    menu: "Show Loop Point Selection Buttons"
    full: "Show help in help pane."
  }
  is_global_setter: true
  address {
    part {
      name: "show_selection_buttons"
    }
  }
  setter_type_name: "rec.widget.waveform.WaveformProto"
}
command {
  type: TOGGLE_SHOW_TIMES_AT_TOP
  category: "Display"
  desc {
    menu: "Show Grid Times At Top"
    full: "Show the grid times for the grid at the top of the waveform display."
  }
  is_global_setter: true
  address {
    part {
      name: "show_times_at_top"
    }
  }
  setter_type_name: "rec.widget.waveform.WaveformProto"
}
command {
  type: TOGGLE_START_STOP
  category: "Transport"
  desc {
    menu: "Toggle Start/Stop"
    full: "Start or pause"
  }
  keypress: "spacebar"
  keypress: "return"
}
command {
  type: TOGGLE_STRETCH_ENABLE
  category: "Audio"
  desc {
    menu: "Enable Stretch"
    full: "Toggle between stretched and real-time playback."
  }
  is_setter: true
  address {
    part {
      name: "time_enabled"
    }
  }
  setter_type_name: "rec.audio.stretch.Stretch"
  keypress: "s"
}
command {
  type: TOGGLE_TRANSFORM_ENABLE
  category: "Audio"
  desc {
    menu: "Enable Transforms"
    full: "Toggle pitch, time and stereo transforms of the sound."
  }
  is_setter: true
  address {
    part {
      name: "enabled"
    }
  }
  setter_type_name: "rec.audio.stretch.Stretch"
}
command {
  type: TOGGLE_WHOLE_SONG_LOOP
  category: "Selection"
  desc {
    menu: "Loop This Segment"
    full: "Toggle selection between the current segment and the whole song."
  }
  keypress: "w"
}
command {
  type: UNDO
  category: "Edit"
  desc {
    menu: "Undo"
    full: "Undoes a previous command."
  }
  keypress: "command + z"
}
