command {
  type: WHATS_NEW_PAGE
  category: "Help"
  desc {
    menu: "What\'s New In This Release?"
    full: "Go to the webpage which tells you what new features and bug fixes are in the release you\'re running."
  }
}
command {
  type: ZOOM_OUT
  category: "Audio"
  desc {
    menu: "Zoom Out"
    full: "Zoom out."
  }
  keypress: "command + -"
}
command {
  type: ZOOM_OUT_FULL
  category: "Audio"
  desc {
    menu: "Zoom Out Completely"
    full: "Zoom out as far as possible."
  }
  keypress: "command + shift + -"
}
command {
  type: ZOOM_TO_SELECTION
  category: "Audio"
  desc {
    menu: "Zoom To Selection"
    full: "Zoom to selection."
  }
  keypress: "command  + \'"
  keypress: "numpad *"
}
