layout {
  name: "NavigationPanel"
  default_size {
    fixed: "RESIZER_SIZE"
  }

  component {
    name: "TreeView"
    tooltip: "CD Window:  Any CDs that you have in your computer's CD drives will appear here."
    custom {
      type_name: "TreeView"
    }
    size {
      min: "75.0"
      max: "-1.0"
      preferred: "-0.2"
    }
  }

  component {
    resizer: "directory_x"
  }

  component {
    layout: "SongData"
    size {
      min: "150.0"
      max: "-1.0"
      preferred: "-0.30"
    }
  }

  component {
    resizer: "metadata_x"
  }

  component {
    name: "Loops"
    custom {
      type_name: "Loops"
    }
    size {
      min: "250.0"
      max: "-1.0"
      preferred: "-0.30"
    }
  }
}
